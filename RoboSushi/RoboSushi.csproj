<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <StartupObject>Develeon64.RoboSushi.RoboSushi</StartupObject>
    <RootNamespace>Develeon64.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Discord.Net" Version="3.8.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="TimeSpan2.Core" Version="2.4.2" />
    <PackageReference Include="TwitchLib" Version="3.4.1" />
    <PackageReference Include="VerboseTimespan" Version="1.0.1" />
  </ItemGroup>

</Project>
